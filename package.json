{
  "name": "blockchain",
  "version": "1.0.0",
  "description": "Learn-by-doing blockchain: modular consensus (PoW/PoS), pluggable crypto (secp256k1 / Falcon PQC), P2P gossip, Merkle trees, blocks, state, and a tiny REST API.",
  "scripts": {
    "start": "node src/index.js",
    "start:secp": "cross-env CRYPTO_MODE=secp256k1 node src/index.js",
    "start:falcon": "cross-env CRYPTO_MODE=falcon node src/index.js",
    "start:pow": "cross-env CONSENSUS_MODE=pow node src/index.js",
    "start:pos": "cross-env CONSENSUS_MODE=pos node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --runInBand --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ac12644/blockchain-secp-falcon"
  },
  "keywords": [
    "blockchain",
    "nodejs",
    "falcon",
    "secp256k1",
    "cryptography",
    "p2p",
    "pow",
    "pos"
  ],
